    ROW_NUMBER() OVER (ORDER BY motor.`id_motor`) AS motor_id_motor,
     motor.`merk` AS motor_merk,
     motor.`harga` AS motor_harga,
     motor.`tahun_produksi` AS motor_tahun_produksi,
